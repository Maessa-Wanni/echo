{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Debugger: Server",
      "type": "debugpy",
      "request": "launch",
      "module": "dembrane.main",
      "cwd": "${workspaceFolder}/server"
    },
    {
      "name": "Attach to Python Functions",
      "type": "debugpy",
      "request": "attach",
      "connect": {
        "host": "localhost",
        "port": 9091
      },
      "preLaunchTask": "func: host start"
    }
  ]
}
